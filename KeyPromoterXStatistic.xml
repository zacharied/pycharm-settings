<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'⌘K'</ShortCut>
          <Description>Commit...</Description>
          <Count>18</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B'</ShortCut>
          <Description>Declaration</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Move to Another Changelist...">
        <Item>
          <IdeaActionID>ChangesView.Move</IdeaActionID>
          <ShortCut>'Alt+Shift+M'</ShortCut>
          <Description>Move to Another Changelist...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘1'</ShortCut>
          <Description>Project</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Push...">
        <Item>
          <IdeaActionID>Vcs.Push</IdeaActionID>
          <ShortCut>'⇧⌘K'</ShortCut>
          <Description>Push...</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Python Console">
        <Item>
          <IdeaActionID>ActivatePythonConsoleToolWindow</IdeaActionID>
          <ShortCut>'⌘5'</ShortCut>
          <Description>Python Console</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
      <Statistic Action="Replace...">
        <Item>
          <IdeaActionID>Replace</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Replace...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'⌥⌘Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Diff">
        <Item>
          <IdeaActionID>Diff.ShowDiff</IdeaActionID>
          <ShortCut>'⌘D'</ShortCut>
          <Description>Show Diff</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'⌘6'</ShortCut>
          <Description>TODO</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌘4'</ShortCut>
          <Description>Terminal</Description>
          <Count>28</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'⌘9' or '⇧⌘9'</ShortCut>
          <Description>Version Control</Description>
          <Count>33</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>